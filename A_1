#include<bits/stdc++.h>

using namespace std;
void f(int xval)
{
	int x;
	x = xval;
	cout << &x << endl;
}
void g(int yval)
{
	int y;
	cout << &y << endl;
}
int main()
{
	f(7);
	g(11);
	return 0;
}

 //Nhận thấy chúng có địa chỉ giống nhau => do chúng đều là biến cục bộ và có thứ tự giống nhau 
 //trong 2 hàm tham số truyền giống nhau 
